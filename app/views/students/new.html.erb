<h1> Student Form </h1>
<%= form_tag students_path do %>
  <label>Student First name:</label><br>
  <%= text_field_tag :'student[first_name]', nil, id: 'first_name' %><br>

  <label>Student Last name:</label><br>
  <%= text_field_tag :'student[last_name]', nil, id: 'last_name' %><br>

 <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag "Submit Student" %>
<% end %>
